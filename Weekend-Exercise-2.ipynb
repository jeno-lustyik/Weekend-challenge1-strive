    "#The Inverted Pyramid Challenge\n",
    "def inverted_pyramid(size):\n",
    "    for n in range(1, size+1, 1):\n",
    "        print(f' ' * (size-n), f'#' * n)\n",
    "    return\n",
    "\n",
    "inverted_pyramid(42)"
